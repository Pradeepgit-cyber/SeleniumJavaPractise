
public class corejava {

	public static void main(String[] args)
	{
		int num = 5;
		String name = "Pradeep";
		char single = 'p';
		double dec = 1.5;
		boolean mycard = true;
		
		System.out.println(num + " " + "is my new code");
		
		//Arrays
		
		int[] arr = new int[5];
		arr[0] = 1;
		arr[1] = 2;
		arr[2] = 3;
		arr[3] = 4;
		arr[4] = 5;
		
		System.out.println(arr[2]);
		
		//for loop
		
		for (int i = 0; i<arr.length; i++)
		{
			System.out.println(arr[i]);
		}
		
		String[] name1 = {"pradeep", "Test", "Autoimation"};
		
		for(int i =0;i<name1.length;i++)
		{
			System.out.println(name1[i]);
		}
	


	}

}
